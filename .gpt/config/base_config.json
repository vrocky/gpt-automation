{
    "extends": "none",
    "override": false,
    "plugins": [
        {
            "plugin_name": "IgnorePlugin",
            "package_name": "gpt_automation",
            "enable": true,
            "settings": {"ignore_filenames": [".gitignore", ".gptignore"]}
        },
        {
            "plugin_name": "IncludeOnlyPlugin",
            "package_name": "gpt_automation",
            "enable": true,
            "settings": {"include_only_filenames": [".gptincludeonly"]}
        }
    ]
  }

